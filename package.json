{
  "name": "bobs-corn-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Express + SQLite (Knex) backend for Bob's Corn storefront",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx server/index.ts",
    "start:prod": "node dist/server/index.js",
    "dev": "tsx watch server/index.ts",
    "seed": "tsx server/seed.ts --products=60 --users=15 --orders=20",
    "seed:reset": "tsx server/seed.ts --reset --products=80 --users=25 --orders=40",
    "seed:dry": "tsx server/seed.ts --dry --products=10 --users=3 --orders=5",
    "typecheck": "tsc --noEmit",
    "prepare": "node -e \"console.log('Ready')\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "express-validator": "7.2.1",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "knex": "3.1.0",
    "sqlite3": "5.1.7",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
